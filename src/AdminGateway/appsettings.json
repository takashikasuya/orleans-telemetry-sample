{
  "TelemetryIngest": {
    "_comment": "Mirror of SiloHost ingest config for Admin UI display",
    "Enabled": [
      "RabbitMq"
    ],
    "BatchSize": 100,
    "ChannelCapacity": 10000,
    "EventSinks": {
      "_comment": "Enabled event sinks (Logging/ParquetStorage). Empty or omitted means all registered sinks run.",
      "Enabled": [
        "Logging",
        "ParquetStorage"
      ]
    }
  },
  "TelemetryStorage": {
    "StagePath": "storage/stage",
    "ParquetPath": "storage/parquet",
    "IndexPath": "storage/index",
    "BucketMinutes": 15,
    "CompactionIntervalSeconds": 300,
    "DefaultQueryLimit": 100000
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
