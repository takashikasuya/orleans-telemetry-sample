# ============================================
# POINTS: HVAC Temperature Sensor
# ============================================
<urn:point-hvac-temp> a sbco:PointExt ;
    sbco:id "point-hvac-temp" ;
    sbco:name "HVAC Supply Temperature" ;
    rec:description "Discharge air temperature from HVAC unit" ;
    sbco:pointType "Temperature" ;
    sbco:unit "celsius" ;
    sbco:aggregate <https://www.sbco.or.jp/ont/AggregateEnum#average> ;
    brick:isPointOf <urn:equipment-hvac-f1> ;
    rec:identifiers (
        [ a sbco:KeyStringMapEntry ;
            sbco:key "point_code" ;
            sbco:value "TEMP-F1-001" ]
    ) ;
    rec:customProperties (
        [ a sbco:KeyMapOfStringMapEntry ;
            sbco:key "sensor_info" ;
            sbco:value (
                [ a sbco:KeyStringMapEntry ;
                    sbco:key "sensor_type" ;
                    sbco:value "PT100_RTD" ]
                [ a sbco:KeyStringMapEntry ;
                    sbco:key "accuracy" ;
                    sbco:value "±0.5°C" ]
            ) ]
    ) ;
    sbco:customTags (
        [ a sbco:KeyBoolMapEntry ;
            sbco:key "is_writable" ;
            sbco:value false ]
    ) .

