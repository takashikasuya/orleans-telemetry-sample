@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rec: <http://www.w3.org/ns/lode/> .
@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix sbco: <https://www.sbco.or.jp/ont/> .

# ============================================
# SITE
# ============================================
<urn:site-tokyo> a sbco:Site ;
    sbco:id "site-tokyo" ;
    sbco:name "Tokyo Technology Center" ;
    rec:description "Main office and R&D facility in Tokyo" ;
    rec:identifiers (
        [ a sbco:KeyStringMapEntry ;
            sbco:key "site_code" ;
            sbco:value "SITE-001" ]
        [ a sbco:KeyStringMapEntry ;
            sbco:key "location" ;
            sbco:value "Tokyo, Japan" ]
    ) ;
    rec:customTags (
        [ a sbco:KeyBoolMapEntry ;
            sbco:key "is_primary" ;
            sbco:value true ]
    ) .

# ============================================
# BUILDING 1: Main Office
# ============================================
<urn:building-main> a sbco:Building ;
    sbco:id "building-main" ;
    sbco:name "Main Office Building" ;
    rec:description "Primary office building with 5 floors" ;
    rec:isPartOf <urn:site-tokyo> ;
    rec:identifiers (
        [ a sbco:KeyStringMapEntry ;
            sbco:key "building_code" ;
            sbco:value "BLD-001" ]
        [ a sbco:KeyStringMapEntry ;
            sbco:key "address" ;
            sbco:value "1-2-3 Chiyoda, Tokyo" ]
    ) ;
    rec:customProperties (
        [ a sbco:KeyMapOfStringMapEntry ;
            sbco:key "construction" ;
            sbco:value (
                [ a sbco:KeyStringMapEntry ;
                    sbco:key "year" ;
                    sbco:value "2015" ]
                [ a sbco:KeyStringMapEntry ;
                    sbco:key "material" ;
                    sbco:value "steel_concrete" ]
            ) ]
    ) .

